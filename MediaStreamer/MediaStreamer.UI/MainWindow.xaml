<Window x:Class="MediaStreamer.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Interop="clr-namespace:System.Windows.Interop;assembly=PresentationCore" 
        xmlns:local="clr-namespace:MediaStreamer.UI"
        mc:Ignorable="d"
        Title="MediaStreamer.UI - MainWindow"
        MinWidth="1200"
        Width="1280"
        Height="830"
        MinHeight="420"
        Loaded="Window_Loaded"
		Background="LightSteelBlue"
		Icon="Images/bjn_logo.ico"
        AutomationProperties.AutomationId="MainWindow" Activated="Window_Activated"
        Closed="Window_Closed" SizeChanged="Window_SizeChanged" StateChanged="Window_StateChanged">
    <Window.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <AlternationConverter x:Key="AlternationConverter"/>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

            <Style TargetType="Button" BasedOn="{StaticResource FlatButtonContactsRight}" x:Key="AnimatedContactsButton">
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Trigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Width" From="80" To="90" Duration="0:0:1" AutoReverse="True" RepeatBehavior="Forever" />
                                    <ColorAnimation Storyboard.TargetProperty="Background.(SolidColorBrush.Color)" From="Black" To="Transparent" Duration="0:0:5" AutoReverse="True" RepeatBehavior="Forever" />
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.EnterActions>
                        <Trigger.ExitActions>
                            <BeginStoryboard>
                                <Storyboard FillBehavior="Stop">
                                    <DoubleAnimation Storyboard.TargetProperty="Width" To="80" Duration="0:0:0.3" />
                                    <ColorAnimation Storyboard.TargetProperty="Background.(SolidColorBrush.Color)" To="Black" Duration="0:0:0.3" />
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.ExitActions>
                    </Trigger>
                </Style.Triggers>
            </Style>

        </ResourceDictionary>
    </Window.Resources>

    <Grid Style="{DynamicResource Font-Proxima-Nova}" x:Name="gridTopWrapper">
        <Grid.Background>
            <ImageBrush Stretch="UniformToFill" >
                <ImageBrush.RelativeTransform>
                    <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="-1" />
                </ImageBrush.RelativeTransform>
                <ImageBrush.ImageSource>
                    <Interop:D3DImage x:Name="mainStreamImage"/>
                </ImageBrush.ImageSource>
            </ImageBrush>
        </Grid.Background>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" MinHeight="100" Margin="15,15,15,0" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <StackPanel Grid.Column="0" Orientation="Vertical" HorizontalAlignment="Left" VerticalAlignment="Top">
                <Label FontSize="24px" FontWeight="Bold" Style="{DynamicResource Font-Proxima-Nova-Bold}" x:Name="labelTitle">
                    <Label.Effect>
                        <DropShadowEffect Color="White" Direction="0" BlurRadius="3" ShadowDepth="3"/>
                    </Label.Effect> BlueJeans Design Sprint
                </Label>
                <Label FontSize="18px" x:Name="labelTime">00:00</Label>
                <Label FontSize="18px" Foreground="Red" x:Name="mainStreamFpsLabel">FPS</Label>
                <Label x:Name="camHwEnabled" Style="{DynamicResource HwAcceleratorLabel}"
                       Visibility="Hidden">
                    HW
                </Label>
            </StackPanel>
            <!--Top toolbar area-->
            <Grid Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Center" Visibility="Hidden">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0" Grid.ColumnSpan="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>

                    <Button Grid.Column="0" Style="{DynamicResource FlatButtonCenter}"
                            local:ButtonExtension.CornerRadius="2,0,0,2">
                        <StackPanel Orientation="Vertical">
                            <Image Source="{DynamicResource VideoIcon}" Style="{DynamicResource ButtonImageCenter}" />
                            <Label>VIDEO</Label>
                        </StackPanel>
                    </Button>

                    <Grid Background="White" Grid.Column="1">
                        <Rectangle Height="60" Width="2" Style="{DynamicResource ButtonCenterSpace}" />
                    </Grid>

                    <Button Grid.Column="2" Style="{DynamicResource FlatButtonCenter}">
                        <StackPanel Orientation="Vertical">
                            <Image Source="{DynamicResource AudioIcon}" Style="{DynamicResource ButtonImageCenter}" />
                            <Label>AUDIO</Label>
                        </StackPanel>
                    </Button>

                    <Grid Background="White" Grid.Column="3">
                        <Rectangle Height="60" Width="2" Style="{DynamicResource ButtonCenterSpace}" />
                    </Grid>

                    <Button Grid.Column="4" Style="{DynamicResource FlatButtonCenter}">
                        <StackPanel Orientation="Vertical">
                            <Image Source="{DynamicResource ScreenShareIcon}" Style="{DynamicResource ButtonImageCenter}" />
                            <Label>SHARE</Label>
                        </StackPanel>
                    </Button>

                    <Button Grid.Column="5" Style="{DynamicResource FlatButtonCenterRed}"
                            local:ButtonExtension.CornerRadius="0,2,2,0" Click="Button_Click_1">
                        <StackPanel Orientation="Vertical">
                            <Image Source="{DynamicResource LeaveCallIcon}" Style="{DynamicResource ButtonImageCenter}" />
                            <Label>LEAVE</Label>
                        </StackPanel>
                    </Button>
                </Grid>

                <ComboBox Grid.Column="2" Margin="10,0,0,0" Style="{DynamicResource FlatComboBox}" Width="84">
                    <ComboBoxItem IsSelected="True">
                        <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                            <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
                                <Image Source="{DynamicResource ContactsIcon}" Style="{DynamicResource ButtonImageCenter}" />
                                <Label HorizontalAlignment="Center">...</Label>
                            </StackPanel>
                        </Grid>
                    </ComboBoxItem>
                </ComboBox>

                <!-- Button Grid.Column="2" Style="{DynamicResource FlatButtonCenter}" Margin="10,0,0,0"
                        local:ButtonExtension.CornerRadius="4">
                    
                </-->
            </Grid>
            <!--Right-Top status icons-->
            <Grid Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Top" MinWidth="220">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>

